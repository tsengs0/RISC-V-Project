#! /usr/bin/vvp
:ivl_version "10.1 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 10;
:vpi_module "system";
:vpi_module "vhdl_sys";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x558062ac6820 .scope module, "tb_pc" "tb_pc" 2 5;
 .timescale -9 -10;
v0x558062ae6d70_0 .net "ce", 0 0, v0x558062ac3190_0;  1 drivers
v0x558062ae6e30_0 .net "pc_addr", 31 0, v0x558062ac3290_0;  1 drivers
v0x558062ae6f00_0 .var "rstn", 0 0;
v0x558062ae7000_0 .var "sys_clk", 0 0;
S_0x558062ac69a0 .scope module, "u0" "pc" 2 11, 3 3 0, S_0x558062ac6820;
 .timescale -9 -10;
    .port_info 0 /INPUT 1 "sys_clk"
    .port_info 1 /INPUT 1 "rstn"
    .port_info 2 /OUTPUT 32 "pc_addr"
    .port_info 3 /OUTPUT 1 "ce"
v0x558062ac3190_0 .var "ce", 0 0;
v0x558062ac3290_0 .var "pc_addr", 31 0;
v0x558062ae6b90_0 .net "rstn", 0 0, v0x558062ae6f00_0;  1 drivers
v0x558062ae6c30_0 .net "sys_clk", 0 0, v0x558062ae7000_0;  1 drivers
E_0x558062a88870/0 .event edge, v0x558062ae6b90_0;
E_0x558062a88870/1 .event posedge, v0x558062ae6c30_0;
E_0x558062a88870 .event/or E_0x558062a88870/0, E_0x558062a88870/1;
    .scope S_0x558062ac69a0;
T_0 ;
    %wait E_0x558062a88870;
    %load/vec4 v0x558062ae6b90_0;
    %cmpi/e 0, 0, 1;
    %jmp/0xz  T_0.0, 4;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x558062ac3190_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %pushi/vec4 1, 0, 1;
    %assign/vec4 v0x558062ac3190_0, 0;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x558062ac69a0;
T_1 ;
    %wait E_0x558062a88870;
    %load/vec4 v0x558062ae6b90_0;
    %pushi/vec4 0, 0, 1;
    %cmp/e;
    %flag_get/vec4 4;
    %load/vec4 v0x558062ac3190_0;
    %pushi/vec4 0, 0, 1;
    %cmp/e;
    %flag_get/vec4 4;
    %and;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.0, 8;
    %pushi/vec4 0, 0, 32;
    %assign/vec4 v0x558062ac3290_0, 0;
    %jmp T_1.1;
T_1.0 ;
    %load/vec4 v0x558062ac3290_0;
    %addi 4, 0, 32;
    %assign/vec4 v0x558062ac3290_0, 0;
T_1.1 ;
    %jmp T_1;
    .thread T_1;
    .scope S_0x558062ac6820;
T_2 ;
    %vpi_call 2 19 "$dumpfile", "tb_pc.vcd" {0 0 0};
    %vpi_call 2 20 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x558062ac6820 {0 0 0};
    %vpi_call 2 22 "$monitor", "%t: pc_addr = %b, ce = %h", $time, v0x558062ae6e30_0, v0x558062ae6d70_0 {0 0 0};
    %end;
    .thread T_2;
    .scope S_0x558062ac6820;
T_3 ;
    %delay 0, 0;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x558062ae6f00_0, 0;
    %delay 1000, 0;
    %pushi/vec4 1, 0, 1;
    %assign/vec4 v0x558062ae6f00_0, 0;
    %delay 3000, 0;
    %vpi_call 2 32 "$finish" {0 0 0};
    %end;
    .thread T_3;
    .scope S_0x558062ac6820;
T_4 ;
    %delay 0, 0;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x558062ae7000_0, 0;
T_4.0 ;
    %delay 100, 0;
    %load/vec4 v0x558062ae7000_0;
    %inv;
    %assign/vec4 v0x558062ae7000_0, 0;
    %jmp T_4.0;
    %delay 3000, 0;
    %vpi_call 2 41 "$finish" {0 0 0};
    %end;
    .thread T_4;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "tb_pc.v";
    "pc.v";
